config dnsmasq
	option domainneeded '1'
	option boguspriv '1'
	option localise_queries '1'
	option expandhosts '1'
	option authoritative '1'
	option readethers '1'
	option leasefile '/tmp/dhcp.leases'
	option noresolv '1'
	option rebind_protection '0'
	option local '/stuker/'
	option domain 'stuker'

config dhcp 'lan'
	option interface 'lan'
	option start '32'
	option limit '150'
	option force '1'
	option leasetime '12h'
	option dhcpv6 'hybrid'
	option ra 'hybrid'
	option ndp 'hybrid'

config dhcp 'wan'
	option interface 'wan'
	option ignore '1'

config dhcp 'tor'
	option interface 'tor'
	option start '100'
	option limit '50'
	option leasetime '1h'
